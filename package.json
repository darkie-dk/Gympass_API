    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.0.1",
    "eslint": "8.57.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.16.0",
    "supertest": "7.0.0",
    "tsup": "8.1.0",
    "tsx": "4.15.7",
    "typescript": "5.5.2",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.1"
  },
